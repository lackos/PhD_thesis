@article{Milstein2002,
abstract = {The self-energy and the vertex radiative corrections to the effect of parity nonconservation in heavy atoms are calculated analytically in orders Z2 and Z23ln(C/r0), where C and r0 are the Compton wavelength and the nuclear radius, respectively. The sum of the radiative corrections is ?0.85% for Cs and ?1.41% for Tl. Using these results, we have performed analysis of the experimental data on atomic parity nonconservation. The values obtained for the nuclear weak charge, QW = ?72.90(28)exp(35)theor for Cs, and QW = ?116.7(1.2)exp(3.4)theor for Tl, agree with predictions of the standard model. As an application of our approach, we have also calculated analytically the dependence of the Lamb shift on the finite size of the nucleus.},
archivePrefix = {arXiv},
