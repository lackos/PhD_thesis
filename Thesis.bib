pages = {1--5},
primaryClass = {physics},
title = {{On the use of the $V^{N-M}$ approximation in atomic calculations}},
url = {http://arxiv.org/abs/physics/0501032%0Ahttp://dx.doi.org/10.1103/PhysRevA.71.032512},
